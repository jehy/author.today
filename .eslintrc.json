{
    "extends": [
        "airbnb-base",
        "airbnb-typescript/base"
    ],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "rules": {
        "no-plusplus": "off",
        "no-await-in-loop": "off",
        "no-mixed-operators": "off",
        "@typescript-eslint/consistent-type-imports": ["error", { "prefer": "type-imports" }]
    }
}
